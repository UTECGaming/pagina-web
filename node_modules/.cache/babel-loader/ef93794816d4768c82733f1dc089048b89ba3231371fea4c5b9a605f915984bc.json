{"ast":null,"code":"import ProductDetails from './ProductDetails.vue';\nexport default {\n  name: 'ProductSlider',\n  data() {\n    return {\n      slideIndex: 0,\n      count: 0,\n      active: 1,\n      data: {\n        images: [\"product1\", \"product2\", \"product3\"],\n        names: [\"Skywalker's R2-D2\", \"Han Solo's Blaster\", \"Poe Dameron's BB-8\"],\n        prices: [\"6.599\", \"2.259\", \"5.999\"],\n        optionTitles: [[\"Anakin\", \"Luke\"], [\"50 Meters\", \"75 Meters\"], [\"T-70X-Wing\", \"T-650X-Wing\"]],\n        progress: [80, 25, 100],\n        backgrounds: [\"background1\", \"background2\", \"background3\"]\n      }\n    };\n  },\n  components: {\n    ProductDetails\n  },\n  methods: {\n    updateCount() {\n      const target = this.data.progress[this.slideIndex];\n      if (this.count < target) {\n        this.count++;\n        setTimeout(this.updateCount, 30);\n      } else {\n        this.count = target;\n      }\n    },\n    slideLeft() {\n      this.slideIndex--;\n      if (this.slideIndex < 0) {\n        this.slideIndex = this.data.names.length - 1;\n      }\n    },\n    slideRight() {\n      this.slideIndex++;\n      if (this.slideIndex > this.data.names.length - 1) {\n        this.slideIndex = 0;\n      }\n    }\n  },\n  created() {\n    this.updateCount();\n  }\n};","map":{"version":3,"names":["ProductDetails","name","data","slideIndex","count","active","images","names","prices","optionTitles","progress","backgrounds","components","methods","updateCount","target","setTimeout","slideLeft","length","slideRight","created"],"sources":["/Users/substex/Documents/UTEC/Gaming/utec-gaming/src/components/ProductSlider.vue"],"sourcesContent":["<template>\n    <section class=\"product-slider\">\n        <img src=\"../assets/logo_2.png\" class=\"logo_img\" />\n        <div class=\"card\">\n            <div\n                class=\"panel-1\"\n                :style=\"'background: url(../assets/' + data.backgrounds[slideIndex] + '.jpg)'\"\n        